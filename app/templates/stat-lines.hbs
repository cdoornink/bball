<div class='stat-lines'>
  <div class="top-row">
    <div class="col player-number">#</div>
    <div class="col player-name">player</div>
    <div class="col min">min</div>
    <div class="col points">pts</div>
    <div class="col totreb">reb</div>
    <div class="col assist">ast</div>
    <div class="col fg">fgm-a</div>
    <div class="col threept">3pm-a</div>
    <div class="col ft">ftm-a</div>
    <div class="col offreb">oreb</div>
    <div class="col defreb">dreb</div>
    <div class="col steal">stl</div>
    <div class="col block">blk</div>
    <div class="col to">to</div>
    <div class="col foul">pf</div>
    <div class="col plusminus">+/-</div>
  </div>
  {{#if left.myTeam}}
    {{#each player in left.players}}
      <div class="player-row">
        <div class="col player-number">{{player.number}}</div>
        <div class="col player-name">{{player.firstName}} {{player.lastName}}</div>
        <div class="col min">min</div>
        <div class="col points">{{player.gameStats.points}}</div>
        <div class="col totreb">{{player.gameStats.reb}}</div>
        <div class="col assist">{{player.gameStats.assists}}</div>
        <div class="col fg">{{player.gameStats.fgm}}/{{player.gameStats.fga}}</div>
